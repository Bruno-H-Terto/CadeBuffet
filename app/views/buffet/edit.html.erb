<h2>Edite seu Buffet</h2>
<% if @buffet.errors.any? %>
  <p>Reveja os campos abaixo: </p>
  <ul>
  <% @buffet.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
  </ul>
<% end %>


  <%= render partial: 'form', locals: { buffet: @buffet, url: buffet_path, method: :patch } %>

